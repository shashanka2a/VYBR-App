modules = ["nodejs-20", "web", "python-3.11"]
[agent]
expertMode = true
integrations = ["web_scraper:1.0.0"]

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 34261
externalPort = 3000

[deployment]
deploymentTarget = "autoscale"
run = ["npx", "serve", "-s", "out", "-l", "5000"]
build = ["npm", "run", "export"]
